 PROC FORMAT FMTLIB;
  ***********************************************;
  *  Create SAS informats for missing values     ;
  ***********************************************;
  INVALUE N2PF
    '-9' = .
    '-8' = .A
    '-7' = .B
    '-6' = .C
    OTHER = (|2.|)
  ;
  INVALUE N3PF
    '-99' = .
    '-88' = .A
    '-77' = .B
    '-66' = .C
    OTHER = (|3.|)
  ;
  INVALUE N4PF
    '-999' = .
    '-888' = .A
    '-777' = .B
    '-666' = .C
    OTHER = (|4.|)
  ;
    INVALUE N5PF
      '-9999' = .
      '-8888' = .A
      '-7777' = .B
      '-6666' = .C
      OTHER = (|5.|)
    ;
    INVALUE N6PF
      '-99999' = .
      '-88888' = .A
      '-77777' = .B
      '-66666' = .C
      OTHER = (|6.|)
    ;
    INVALUE N6P2F
      '-99.99' = .
      '-88.88' = .A
      '-77.77' = .B
      '-66.66' = .C
      OTHER = (|6.2|)
    ;
    INVALUE N7P2F
      '-999.99' = .
      '-888.88' = .A
      '-777.77' = .B
      '-666.66' = .C
      OTHER = (|7.2|)
    ;
      INVALUE N10PF
        '-999999999' = .
        '-888888888' = .A
        '-777777777' = .B
        '-666666666' = .C
        OTHER = (|10.|)
      ;
      INVALUE DATE10F
        '-999999999' = .
        '-888888888' = .A
        '-777777777' = .B
        '-666666666' = .C
        OTHER = (|MMDDYY10.|)
      ;
      INVALUE N12P2F
        '-99999999.99' = .
        '-88888888.88' = .A
        '-77777777.77' = .B
        '-66666666.66' = .C
        OTHER = (|12.2|)
      ;
      INVALUE N15P2F
        '-99999999999.99' = .
        '-88888888888.88' = .A
        '-77777777777.77' = .B
        '-66666666666.66' = .C
        OTHER = (|15.2|)
      ;

      RUN;
